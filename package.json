{
  "name": "rocket",
  "version": "2.1.1",
  "description": "Rocket is a baseline SASS proyect to make an standard CSS for any proyect with your own prefix if u want it",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install": "npm i node-sass",
    "start": "node-sass --watch src/scss -o src/css",
    "build": "node-sass src/scss -o src/css --output-style compressed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://rholo@bitbucket.org/rholo/rocket.git"
  },
  "author": "Rodrigo Ahumada",
